{
  "manifest_version": 3,
  "name": "R Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "js/index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8282/*",
    "https://cdn.jsdelivr.net/npm/*"
  ]}